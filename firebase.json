{
	"functions": {
		"predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"],
		"source": "functions"
	},
	"hosting": {
		"public": "./client/build",
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html",
				"function": "app"
			}
		],
		"ignore": ["**/.*", "**/node_modules/**"]
	}
}
